{
  "name": "efemerides",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devhost": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite build && vite preview",
    "servehost": "vite build && vite preview --host"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
